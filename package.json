{
  "name": "api-rest",
  "version": "0.1.0",
  "homepage": "https://github.com/quienmedebe/api-rest#readme",
  "description": "API REST from Quién Me Debe application",
  "main": "./src/app.js",
  "type": "commonjs",
  "engines": {
    "node": ">=14.11.0"
  },
  "author": "Raúl Contreras Rubio",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quienmedebe/api-rest.git"
  },
  "bugs": {
    "url": "https://github.com/quienmedebe/api-rest/issues"
  },
  "scripts": {
    "check-package-manager": "node -e \"!process.env.npm_config_user_agent.startsWith('npm/') && !console.log('---- USE NPM TO INSTALL DEPENDENCIES ----') && process.exit(1)\"",
    "preinstall": "npm run check-package-manager --silent",
    "start": "NODE_ENV=production node ./src/bin/www",
    "dev": "NODE_ENV=development nodemon ./src/bin/www",
    "test": "cross-env NODE_ENV=test mocha \"./test/**/*.test.js\"",
    "test:unit": "cross-env NODE_ENV=test mocha \"./test/unit/**/*.test.js\"",
    "test:integration": "cross-env NODE_ENV=test mocha \"./test/integration/**/*.test.js\"",
    "test:coverage": "cross-env NODE_ENV=test nyc --reporter=text mocha \"./test/**/*.test.js\" --exit",
    "test:dump": "cross-env NODE_ENV=test mocha --require ./test/utils/scripts/asyncDump.js \"./test/**/*.test.js\"",
    "docs": "npm run docs:md && npm run docs:html",
    "docs:md": "swagger-markdown -i ./swagger.json -o ./docs/index.md --skip-info",
    "docs:html": "redoc-cli bundle ./swagger.json --output ./docs-html/index.html",
    "dev:db:migrate": "cross-env NODE_ENV=development npx sequelize-cli db:migrate",
    "dev:db:migrate:undo": "cross-env NODE_ENV=development npx sequelize-cli db:migrate:undo",
    "dev:db:migrate:undo:all": "cross-env NODE_ENV=development npx sequelize-cli db:migrate:undo:all",
    "test:db:migrate": "cross-env NODE_ENV=test npx sequelize-cli db:migrate",
    "test:db:migrate:undo": "cross-env NODE_ENV=test npx sequelize-cli db:migrate:undo",
    "test:db:migrate:undo:all": "cross-env NODE_ENV=test npx sequelize-cli db:migrate:undo:all",
    "prod:db:migrate": "cross-env NODE_ENV=production npx sequelize-cli db:migrate",
    "prod:db:migrate:undo": "cross-env NODE_ENV=production npx sequelize-cli db:migrate:undo"
  },
  "husky": {
    "hooks": {
      "pre-commit": "touch .commit && pretty-quick --staged",
      "post-commit": "./scripts/post-commit.sh"
    }
  },
  "dependencies": {
    "ajv": "^6.12.5",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-http-context": "^1.2.4",
    "express-jwt": "^6.0.0",
    "helmet": "^4.1.1",
    "jsonwebtoken": "^8.5.1",
    "lodash.isplainobject": "^4.0.6",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.3.3",
    "pg-hstore": "^2.3.3",
    "rand-token": "^1.0.1",
    "rate-limiter-flexible": "^2.1.10",
    "redis": "^3.0.2",
    "sequelize": "^6.3.5",
    "stoppable": "^1.1.0",
    "swagger-ui-express": "^4.1.4",
    "uuid": "^8.3.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "api-contract-validator": "^2.1.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-mocha": "^8.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-security": "^1.4.0",
    "faker": "^5.1.0",
    "husky": "^4.3.0",
    "mocha": "^8.1.3",
    "nodemon": "^2.0.5",
    "nyc": "^15.1.0",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "redoc-cli": "^0.9.12",
    "sinon": "^9.2.0",
    "swagger-markdown": "^1.4.3",
    "rewire": "^5.0.0",
    "proxyquire": "^2.1.3",
    "sequelize-test-helpers": "^1.3.2",
    "sinon-chai": "^3.5.0"
  }
}
